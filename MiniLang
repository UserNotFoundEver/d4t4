// Define a function to add two numbers
func int add(int a, int b) {
    return a + b;
}

// Main program
func int main() {
    int x = 10;
    int y = 20;
    int sum = add(x, y);
    if (sum > 15) {
        // If the sum is greater than 15, print a message
        print("Sum is greater than 15.");
    } else {
        print("Sum is 15 or less.");
    }
    return 0;
}
